{
  "name": "timezone-compare",
  "version": "1.0.10",
  "bin": {
    "timezone-compare": "./dist/cli.js"
  },
  "keywords": [
    "timezone-compare",
    "timezone",
    "compare",
    "calendar",
    "datetime",
    "gmt",
    "moment",
    "date",
    "time",
    "zone",
    "moment-timezone",
    "node",
    "module"
  ],
  "description": "A cli tool to get a list of times across 2 different timezones for easy checking",
  "main": "dist/index.js",
  "scripts": {
    "patch-release": "npm version patch && npm publish",
    "clean": "rimraf dist",
    "start": "NODE_ENV=development nodemon --exec babel-node src/index.js",
    "build": "npm run clean && BABEL_ENV=production NODE_ENV=production babel src --out-dir dist",
    "test": "ava"
  },
  "repository": "bhargav175/timezone-compare",
  "author": "Bhargav Ponnapalli",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babili": "^0.0.9",
    "dotenv": "^2.0.0",
    "eslint": "^3.9.1",
    "nodemon": "^1.11.0",
    "rimraf": "^2.5.4",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "2.1.0-beta.11"
  },
  "dependencies": {
    "inquirer": "^2.0.0",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.10",
    "table": "^4.0.1"
  },
  "ava": {
    "source": [
      "/src/**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
